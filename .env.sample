# App
ENVIRONMENT=development

# Postgres
DB_HOST=localhost
DB_PORT=5432
DB_NAME=bazaaryar_dev
DB_USER=bazaaryar
DB_PASSWORD=bazaaryar
DB_SSL_MODE=prefer

# Optional: overrides the discrete DB_* settings
DATABASE_URL=postgresql://bazaaryar:bazaaryar@localhost:5432/bazaaryar_dev?sslmode=prefer

# CORS Origins
FRONTEND_ORIGINS=http://localhost:3000,http://127.0.0.1:3000

# Phoenix Observability (Backend)
PHOENIX_ENABLED=false
PHOENIX_PROJECT_NAME=bazaaryar-agent
PHOENIX_COLLECTOR_ENDPOINT=http://localhost:4317

# Phoenix Observability (Infra)
PHOENIX_TAG=latest
PHOENIX_UI_PORT=6006
PHOENIX_GRPC_PORT=4317
PHOENIX_WORKING_DIR=/mnt/data

# Gemini
GEMINI_MODEL=gemini-3-pro-preview
GEMINI_THINKING_LEVEL=high
GOOGLE_API_KEY=

# OpenAI
OPENAI_MODEL=gpt-4.1-mini
OPENAI_API_KEY=

# OpenAI-Compatible
OPENAILIKE_MODEL=gpt-4.1-mini
OPENAILIKE_API_KEY=
OPENAILIKE_BASE_URL=

# Sandbox configs
SANDBOX_TOOL_ENABLED=true
SANDBOX_DOCKER_IMAGE=bazaaryar-python-sandbox:latest
SANDBOX_DOCKER_BIN=docker
SANDBOX_MAX_RUNTIME_SECONDS=90
SANDBOX_MAX_MEMORY_MB=1024
SANDBOX_MAX_CPU=1.0
SANDBOX_MAX_ARTIFACTS=8
SANDBOX_MAX_ARTIFACT_BYTES=10485760
SANDBOX_MAX_CODE_CHARS=20000
SANDBOX_PERSIST_SESSIONS=true
SANDBOX_SESSION_IDLE_TTL_SECONDS=43200
SANDBOX_SESSION_POLL_INTERVAL_MS=100
SANDBOX_SESSION_QUEUE_WAIT_TIMEOUT_SECONDS=180
SANDBOX_SESSION_SWEEP_INTERVAL_SECONDS=60

# Frontend
NEXT_PUBLIC_API_BASE_URL=http://localhost:8000
NEXT_PUBLIC_PHOENIX_URL=http://localhost:6006
